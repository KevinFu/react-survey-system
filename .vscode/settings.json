{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.fixAll.eslint": "always"
  },
  "eslint.validate": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact"
  ],
  "cSpell.words": [
    "ahooks",
    "Appstore",
    "downarrow",
    "Idcard",
    "partialize",
    "uparrow",
    "zundo"
  ],
  "tailwindcss.includeLanguages": {
    "typescript": "typescript",
    "typescriptreact": "typescriptreact"
  }
}
